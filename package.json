{
  "name": "langger",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "node scripts/bump-version.mjs",
    "build": "vite build",
    "preview:https": "vite preview --host --https",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "test:unit": "vitest",
    "test:unit:build": "node scripts/test-after-build.mjs",
    "test:unit:after-build": "vitest --config vitest.config.standard.ts --run",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open --e2e",
    "test:component": "cypress run --component",
    "test:component:open": "cypress open --component",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:component",
    "test:all:build": "npm run test:unit:build && npm run test:e2e && npm run test:component"
  },
  "dependencies": {
    "@sentry/react": "^10.25.0",
    "@sentry/tracing": "^7.120.4",
    "antd": "^5.27.6",
    "dexie": "^4.2.1",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.1.5",
    "react-router-dom": "^7.9.4",
    "react-window": "^2.2.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@cypress/vite-dev-server": "^7.0.1",
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-window": "^1.8.8",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^1.0.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "cypress": "^15.6.0",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.12",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "fake-indexeddb": "^6.2.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "happy-dom": "^12.10.3",
    "jsdom": "^23.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vitest": "^1.0.4"
  },
  "alias": {
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "homepage": "https://github.com/Addyyaa/Lannger",
  "optionalDependencies": {
    "vite": "^7.2.2"
  }
}
